{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "public/dist",
  "installCommand": "npm install && cd public && npm install",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/share/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
