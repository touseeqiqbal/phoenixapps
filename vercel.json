{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "public/dist",
  "installCommand": "npm install && cd public && npm install",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/share/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
